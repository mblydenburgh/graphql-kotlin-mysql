scalar DateTime

type Claim {
    claimId: String!
    lossDate: DateTime
    payments(claimId: String): [Payment]!
}

type Query {
    getAllClaims: [Claim]!
    getClaimById(id: String!): Claim
}

extend type Mutation {
    newClaim(claimId: String!, lossDate: String!): Claim!
    removeClaim(claimId: String!): Int
}